{
  "name": "site",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "apps/*",
    "packages/*",
    "./tests"
  ],
  "scripts": {
    "service-start": "NODE_ENV=production pm2 start ecosystem.config.js && yarn monit",
    "service-start-dev": "yarn prepare-dev && node prepare.js && yarn pre-build && yarn service-start-dev-fast",
    "dev": "yarn service-start-dev",
    "prod": "yarn service-start",
    "dev-all": "FULLDEV=1 yarn dev",
    "dev-latest": "yarn service-start-dev-latest",
    "dev-fast": "yarn service-start-dev-fast",
    "dev-all-fast": "FULLDEV=1 yarn service-start-dev-fast",
    "service-start-dev-latest": "yarn prepare-dev && node prepare.js --latest && yarn pre-build && yarn service-start-dev-fast",
    "service-start-dev-fast": "pm2 start ecosystem.config.js && yarn monit",
    "start": "yarn start-dev",
    "start-prod": "NODE_ENV=production pm2 start ecosystem.config.js --no-daemon",
    "start-dev": "yarn prepare-dev && node prepare.js && yarn pre-build && yarn start-dev-fast",
    "start-dev-all": "FULLDEV=1 yarn start-dev",
    "start-dev-all-fast": "FULLDEV=1 yarn start-dev-fast",
    "start-dev-latest": "yarn prepare-dev && node prepare.js --latest && yarn pre-build && yarn start-dev-fast",
    "start-dev-fast": "pm2 start ecosystem.config.js --no-daemon",
    "pre-build": "yarn && yarn build",
    "prepare-dev": "yarn workspace api prepare-dev && yarn workspaces foreach run prepare-dev",
    "native": "cd apps/expo && yarn start",
    "watch": "yarn workspaces foreach -pi run watch",
    "electron": "cd apps/electron && yarn electron",
    "electron-package": "cd apps/electron && yarn package",
    "electron-package-fast": "cd apps/electron && yarn package-fast",
    "web": "cd apps/next && yarn next",
    "web:extract": "DISABLE_EXTRACTION=false yarn workspace next-app dev",
    "web:build": "yarn workspace next-app build",
    "admin:build": "yarn workspace admin build",
    "docs:build": "yarn workspace nextra build",
    "web:serve": "NODE_ENV=production yarn workspace next-app serve",
    "admin-prod": "yarn workspace admin admin-start",
    "admin-dev": "yarn workspace admin admin-start-dev",
    "proxy-prod": "yarn workspace proxy proxy-start",
    "proxy-dev": "yarn workspace proxy proxy-start-dev",
    "admin-api-prod": "yarn workspace admin-api admin-api-start",
    "admin-api-dev": "yarn workspace admin-api admin-api-start-dev",
    "api-prod": "yarn workspace api api-start",
    "api-dev": "yarn workspace api api-start-dev",
    "docs-dev": "yarn workspace nextra docs-start-dev",
    "docs-prod": "yarn workspace nextra docs-start",
    "add-user": "yarn workspace api add-user",
    "pull": "git pull && yarn workspaces foreach run pull",
    "fix": "manypkg fix",
    "build": "yarn workspaces foreach --exclude next-app --exclude electron-app --exclude nextra --exclude admin run build",
    "package": "yarn prepare-dev && node prepare.js && yarn build && yarn workspaces foreach --exclude electron-app run package",
    "check-deps": "check-dependency-version-consistency .",
    "status": "pm2 status",
    "monit": "pm2 monit",
    "dashboard": "pm2 plus",
    "kill": "pm2 kill",
    "stop": "pm2 stop all",
    "logs": "pm2 logs",
    "create-environment": "node copy.js",
    "test:all:long": "LONG_TESTS=true yarn test:all",
    "test:all": "NODE_ENV=development FORCE_COLOR=1 yarn workspaces foreach run test",
    "test:all:debug": "FORCE_COLOR=1 TEST_DEBUG=true yarn test:all",
    "test:global": "NODE_ENV=development FORCE_COLOR=1 yarn workspace tests test",
    "test:e2egen": "NODE_ENV=development npx playwright codegen",
    "test:protolib": "FORCE_COLOR=1 yarn workspace protolib test"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "^0.18.12"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@manypkg/cli": "^0.19.1",
    "async": "3.2.5",
    "check-dependency-version-consistency": "^3.0.3",
    "eslint": "^8.21.0",
    "fdir": "6.1.1",
    "glob": "^10.3.10",
    "ignore": "5.3.0",
    "mkdirp": "^3.0.1",
    "node-gyp": "^9.3.1",
    "pm2": "^5.3.0",
    "prettier": "^2.8.1",
    "rimraf": "^5.0.1",
    "semver": "^7.5.4",
    "turbo": "^1.10.3",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@3.5.0",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
