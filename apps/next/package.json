{
  "name": "next-dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare-dev": "node prepare.js",
    "dev-standalone": "FULL_DEV=1 NODE_ENV=development node entrypoint.js",
    "dev-service": "FULL_DEV=1 NODE_ENV=development pm2 start pm2.config.js",
    "prod": "NODE_ENV=production pm2 start pm2.config.js --no-daemon",
    "dev": "FULL_DEV=1 NODE_ENV=development pm2 start pm2.config.js --no-daemon",
    "package": "next build && shx cp -r .next/ ../next-compiled && shx cp -r pages/ ../next-compiled && shx cp -r public/ ../next-compiled && shx cp -r ../../data/dev/public/. ../../data/prod/public/",
    "lint": "next lint",
    "test": "jest",
    "test:deps": "npx playwright install && npx playwright install-deps",
    "clean": "shx rm -rf .next"
  },
  "dependencies": {
    "@craftjs/core": "0.2.0-beta.8",
